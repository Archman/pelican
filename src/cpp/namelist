## input parameters example

# input seed field, [V/m], prefix: seed
seedEx = "5e5,0"
seedEy = "0,0"

# undulator, prefix: undulator
undulatorField  = 0.42548
undulatorPeriod = 0.025
undulatorNstep  = 1
undulatorNum    = 500

# electron beam, prefix: electron
electronAvgBetaFunc   = 6
electronCentralEnergy = 264.9
electronEnergySpread  = 265e-4
electronEmitnx        = 4e-6
electronEmitny        = 4e-6
electronPeakCurrent   = 300

# FEL radiation, prefix: FEL
FELwavelength = 265.85e-9

# control panel, prefix: cp
cpnpart   = 80000
cpmethod = "RK4"
#cpmethod = "EU2"
#cpmethod  = "EU1"
cpoutfile = "pelican80k.out"

# scan panel, prefix: sp
spsflag   = 0
speflag   = 1
spobjfunc = "power"
spparam   = "electronCentralEnergy"
spvbegin  = 260
spvend    = 270
spvstep   = 0.5
spoutfile = "pelican.scan"

